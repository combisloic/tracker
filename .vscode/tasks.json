{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "command": "npm",
  "isShellCommand": true,
  "showOutput": "always",
  "suppressTaskName": true,
  "tasks": [
    {
      "taskName": "install",
      "args": ["install"]
    },
    {
      "taskName": "update",
      "args": ["update"]
    },
    {
      "taskName": "test",
      "args": ["run", "test"]
    },
    {
      "taskName": "build",
      "args": ["run", "build"]
    },
    {
      "taskName": "develop",
      "args": ["run", "dev"]
    },
    {
      "taskName": "start",
      "args": ["start"]
    },
    {
      "taskName": "stop",
      "args": ["stop"]
    },
    {
      "taskName": "generate:configs",
      "args": ["run", "generate:configs"]
    },
    {
      "taskName": "docker:build",
      "args": ["run", "docker:build"]
    },
    {
      "taskName": "docker:run:watch",
      "args": ["run", "docker:run:watch"]
    },
    {
      "taskName": "docker:run",
      "args": ["run", "docker:run"]
    }
  ]
}
